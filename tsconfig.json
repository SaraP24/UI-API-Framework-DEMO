{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "outDir": "dist"
  },
  "paths": {
    "@fixtures/*": [
      "./fixtures/*"
    ],
    "@test-data/*": [
      "./test-data/*"
    ],
    "@test-data": [
      "./test-data/index"
    ],
    "@helpers/*": [
      "./helpers/*"
    ],
    "@config/*": [
      "./config/*"
    ],
    "@enums": [
      "./models/enums/index"
    ],
    "@interface/*": [
      "./models/interface/*"
    ],
    "@poms": [
      "./page-objects/pages/index"
    ],
    "@tests/*": [
      "./tests/*"
    ],
    "@test-ui/*": [
      "./tests/ui/*"
    ],
    "@test-api/*": [
      "./tests/api/*"
    ]
  },
  "include": [
    "**/*.ts"
  ],
}